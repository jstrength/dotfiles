[init]
	defaultBranch = main
[user]
	name 	= Jacob Strength
	email 	= jstrength562@gmail.com
[alias]
	co    	= checkout
	ci    	= commit -m
	st    	= status
	br    	= branch
	hist  	= log --pretty=format:\"%h %af | %s%d [%an]\" --graph --date=short
	d     	= diff
	pl    	= pull
	pu    	= push
	a     	= add
	ap    	= add -p
	ac    	= !git add . && git commit -m
	undo  	= reset --soft HEAD~1
[core]
	autocrlf = input
[pull]
	rebase = true
